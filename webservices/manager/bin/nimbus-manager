#!/usr/bin/env python
# -*- coding: UTF-8 -*-

import os
import sys
sys.path.append(os.path.dirname(os.path.dirname(__file__)))
from nimbusmanager import daemon

if __name__ == "__main__":
    if len(sys.argv) == 2 and sys.argv[1] == "--debug":
        daemon.main(debug=True)
    else:
        daemon.main()
